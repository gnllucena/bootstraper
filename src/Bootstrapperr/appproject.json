{
  "Name": "API.Identity",
  "Database": "Mysql",
  "Entities": [
    {
      "Name": "User",
      "Table": "user",
      "PreInserts": [
        {
          "Property": "Created",
          "Set": "DateTime.Now"
        },
        {
          "Property": "CreatedBy",
          "Set": "_authenticatedService.GetUserName()"
        }
      ],
      "PreUpdates": [
        {
          "Property": "Id",
          "Set": "id"
        },
        {
          "Property": "Updated",
          "Set": "DateTime.Now"
        },
        {
          "Property": "UpdatedBy",
          "Set": "_authenticatedService.GetUserName()"
        }
      ],
      "Properties": [
        {
          "Name": "Id",
          "Column": "id",
          "Primitive": "int",
          "IsPrimaryKey": true,
          "IsUnique": false,
          "IsNullable": false
        },
        {
          "Name": "Name",
          "Column": "name",
          "Primitive": "string",
          "IsPrimaryKey": false,
          "IsNullable": false,
          "IsUnique": false,
          "Validations": [
            {
              "Type": "MinimumLength",
              "Value": "1"
            },
            {
              "Type": "MaximumLength",
              "Value": "100"
            },
            {
              "Type": "Required",
              "Value": null
            }
          ]
        },
        {
          "Name": "Email",
          "Column": "email",
          "Primitive": "string",
          "IsPrimaryKey": false,
          "IsNullable": false,
          "IsUnique": true,
          "Validations": [
            {
              "Type": "MinimumLength",
              "Value": "5"
            },
            {
              "Type": "MaximumLength",
              "Value": "100"
            },
            {
              "Type": "Required",
              "Value": null
            },
            {
              "Type": "Email",
              "Value": null
            }
          ]
        },
        {
          "Name": "Password",
          "Column": "password",
          "Primitive": "string",
          "IsPrimaryKey": false,
          "IsNullable": false,
          "IsUnique": false,
          "Validations": [
            {
              "Type": "MinimumLength",
              "Value": "8"
            },
            {
              "Type": "MaximumLength",
              "Value": "30"
            },
            {
              "Type": "Required",
              "Value": null
            }
          ]
        },
        {
          "Name": "Active",
          "Column": "active",
          "Primitive": "bool",
          "IsPrimaryKey": false,
          "IsUnique": false,
          "IsNullable": false
        },
        {
          "Name": "Confirmed",
          "Column": "confirmed",
          "Primitive": "bool",
          "IsPrimaryKey": false,
          "IsUnique": false,
          "IsNullable": false
        },
        {
          "Name": "Created",
          "Column": "created",
          "Primitive": "DateTime",
          "IsPrimaryKey": false,
          "IsNullable": false,
          "IsUnique": false,
          "Validations": [
            {
              "Type": "Required",
              "Value": null
            },
            {
              "Type": "Past",
              "Value": null
            }
          ]
        },
        {
          "Name": "CreatedBy",
          "Column": "createdby",
          "Primitive": "string",
          "IsPrimaryKey": false,
          "IsNullable": false,
          "IsUnique": false,
          "Validations": [
            {
              "Type": "Required",
              "Value": null
            }
          ]
        },
        {
          "Name": "Updated",
          "Column": "updated",
          "Primitive": "DateTime",
          "IsPrimaryKey": false,
          "IsNullable": true,
          "IsUnique": false,
          "Validations": [
            {
              "Type": "Required",
              "Value": null,
              "Depends": {
                "On": "Id",
                "When": "GreaterThanZero"
              }
            },
            {
              "Type": "Past",
              "Value": null,
              "Depends": {
                "On": "Id",
                "When": "GreaterThanZero"
              }
            },
            {
              "Type": "Empty",
              "Value": null,
              "Depends": {
                "On": "Id",
                "When": "EqualTozero"
              }
            }
          ]
        },
        {
          "Name": "UpdatedBy",
          "Column": "updatedby",
          "Primitive": "string",
          "IsPrimaryKey": false,
          "IsNullable": true,
          "IsUnique": false,
          "Validations": [
            {
              "Type": "Required",
              "Value": null,
              "Depends": {
                "On": "Id",
                "When": "GreaterThanZero"
              }
            },
            {
              "Type": "Empty",
              "Value": null,
              "Depends": {
                "On": "Id",
                "When": "EqualToZero"
              }
            }
          ]
        }
      ]
    }
  ]
}